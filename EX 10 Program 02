//Swapping using call by reference
#include <iostream>
using namespace std;

void swap(int *x, int *y)  
{  
    int temp;
    temp = *x;  
    *x = *y;  
    *y = temp;  
}  

int main()   
{
    int a = 20, b = 50;    
    swap(&a, &b);  
    
    cout << "Value of a is: " << a << endl;  
    cout << "Value of b is: " << b << endl;  
    
    return 0;  
}
